CREATE TABLE customers
(
	customer_id varchar(100) PRIMARY KEY,
	company_name varchar(100) NOT NULL,
	contact_name varchar(100) UNIQUE
);

CREATE TABLE employees
(
	employe_id serial UNIQUE,
	first_name varchar(100) NOT NULL,
	last_name varchar(100) NOT NULL,
	title varchar(500),
	birth_date date ,
	notes text
);

CREATE TABLE orders
(
	order_id serial ,
	costomer_id varchar(100) REFERENCES customers(customer_id),
	employe_id int REFERENCES employees(employe_id),
	order_date date,
	ship_city varchar(100)
)
