SELECT ship_city, ship_country
FROM orders
WHERE ship_city like '%burg'

SELECT order_id, customer_id, freight, ship_country
FROM orders
WHERE ship_country LIKE 'P%'
ORDER BY freight DESc
LIMIT 10

SELECT *
FROM EMPLOYEES
WHERE REGION IS NULL

SELECT COUNT(SUPPLIER_ID)
FROM SUPPLIERS
GROUP BY COUNTRY
ORDER BY COUNT(SUPPLIER_ID) DESC

SELECT SUM(FREIGHT)
FROM ORDERS
WHERE SHIP_REGION IS NOT   NULL
GROUP BY SHIP_COUNTRY
HAVING SUM(FREIGHT) > 2750
ORDER BY SUM(FREIGHT) DESC

SELECT COUNTRY
FROM EMPLOYEES
INTERSECT
SELECT COUNTRY
FROM CUSTOMERS
INTERSECT
SELECT COUNTRY
FROM SUPPLIERS

SELECT COUNTRY
FROM CUSTOMERS
INTERSECT
SELECT COUNTRY
FROM SUPPLIERS
EXCEPT
SELECT COUNTRY
FROM EMPLOYEES
